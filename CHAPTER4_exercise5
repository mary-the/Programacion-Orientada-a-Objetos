public class TestSandwichFilling {
	public static void main(String[] args) {
	
		SandwichFilling a =new SandwichFilling("egg salad",500);
		SandwichFilling b =new SandwichFilling("egg salad",500);
		SandwichFilling c =new SandwichFilling("egg salad",500);
		a.getType();
		a.getCalories();
		b.getType();
		b.getCalories();
		c.getType();
		c.getCalories();
	
	}
}
